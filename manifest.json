{
  "name": "Cloudfront Purge Tool",
  "version": "1.1",
  "description": "Easily purge (invalidate) files from Amazon Cloudfront, using an intuitive, secure interface.",
  "manifest_version": 2,
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "app": {
    "launch": {
      "local_path": "popup.html"
    }
  },
  "permissions": [
    "https://cloudfront.amazonaws.com/",
    "notifications"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://www.google.com https://www.cdnplanet.com; object-src 'self';"
}
